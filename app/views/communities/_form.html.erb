<%= form_for @community do |f| %>
  <div class="form-group"><%= f.label :name %><%= f.text_field :name, class: 'form-control' %></div>
  <div class="form-group"><%= f.label :description %><%= f.text_field :description, class: 'form-control' %></div>
  <div class="form-group"><%= f.submit 'Create Community', class: 'btn btn-default' %></div>
<% end %>
