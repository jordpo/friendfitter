<%= form_for workout_session, method: method do |f| %>
<%= f.label :pr %><%= f.check_box :pr %>
<%= f.label :comment %><%= f.text_field :comment %>
<%= f.label :accomplished %><%= f.check_box :accomplished %>
<%= f.label :status %><%= f.text_field :status %>
<%= f.hidden_field :workout_id, value: @workout.id %>
<%= f.submit submit %>
<% end %>
